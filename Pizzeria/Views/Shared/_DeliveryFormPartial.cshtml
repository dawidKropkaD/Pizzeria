@model Pizzeria.ViewModels.DeliveryFormViewModel

<h4>Adres dostawy</h4>
<div class="form-group">
    <label asp-for="City" class="col-md-2 control-label"></label>
    <div class="col-md-10">
        <input asp-for="City" class="form-control" readonly="readonly" />
        <span asp-validation-for="City" class="text-danger"></span>
    </div>
</div>
<div class="form-group">
    <label asp-for="Street" class="col-md-2 control-label"></label>
    <div class="col-md-10">
        <input asp-for="Street" class="form-control" />
        <span asp-validation-for="Street" class="text-danger"></span>
    </div>
</div>
<div class="form-group">
    <label asp-for="HouseNumber" class="col-md-2 control-label"></label>
    <div class="col-md-10">
        <input asp-for="HouseNumber" class="form-control" />
        <span asp-validation-for="HouseNumber" class="text-danger"></span>
    </div>
</div>
<div class="form-group">
    <label asp-for="FlatNumber" class="col-md-2 control-label"></label>
    <div class="col-md-10">
        <input asp-for="FlatNumber" class="form-control" />
        <span asp-validation-for="FlatNumber" class="text-danger"></span>
    </div>
</div>
<h4>@ViewData["Header"]</h4>
<div style="display:@Model.DisplayForMember" class="form-group">
    <label asp-for="ClientName" class="col-md-2 control-label"></label>
    <div class="col-md-10">
        <input asp-for="ClientName" class="form-control" />
        <span asp-validation-for="ClientName" class="text-danger"></span>
    </div>
</div>
<div class="form-group">
    <label asp-for="Phone" class="col-md-2 control-label"></label>
    <div class="col-md-10">
        <input asp-for="Phone" class="form-control" />
        <span asp-validation-for="Phone" class="text-danger"></span>
    </div>
</div>